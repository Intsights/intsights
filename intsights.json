{
  "appid" : "ef0b1f6f-a05a-4f41-8f83-1e659771b6bb",
  "name" : "Intsights",
  "description" : "This app integrates with Intsights Cyber Intelligence.",
  "type" : "threat intel",
  "main_module" : "intsights_connector.py",
  "app_version" : "1.0.0",
  "product_vendor" : "Intsights",
  "product_name" : "Intsights Cyber Intelligence",
  "product_version_regex": ".*",
  "logo": "intsights.png",
  "min_phantom_version": "1.0.240",
  "publisher" : "Blackstone, Inc",
  "package_name": "blackstone_intsights",
  "license": "Copyright (c) Blackstone 2016",
  "consolidate_widgets" : false,
  "configuration": {
    "email": {
      "data_type": "string",
      "description": "User's email address",
      "required": true,
      "order": 0
    },
    "password": {
      "data_type": "password",
      "description": "User's password",
      "required": true,
      "order": 1
    }
  },
  "actions": [
    {
      "action": "test connectivity",
      "identifier": "test_asset_connectivity",
      "description": "Validate the asset configuration for connectivity.",
      "type": "test",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {},
      "output": []
    },
    {
      "action": "hunt file",
      "identifier": "hunt_file",
      "description": "Look for information about a file hash in the Intsights database.",
      "type": "investigate",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {
        "hash": {
          "description": "Hash of the binary to hunt",
          "data_type": "string",
          "contains": ["hash", "sha256", "sha1", "md5"],
          "required": true
        }
      },
      "render": {
        "width": 12,
        "title": "Hunt File",
        "menu_name": "Intsights",
        "type": "table",
        "height": 5
      },
      "output": [
        {
          "data_path": "action_result.data.*.Value",
          "data_type": "string",
          "contains": ["hash"],
          "column_name": "Hash",
          "column_order": 0
        },
        {
          "data_path": "action_result.data.*.Source",
          "data_type": "string",
          "column_name": "Source",
          "column_order": 1
        },
        {
          "data_path": "action_result.data.*.Description",
          "data_type": "string",
          "column_name": "Description",
          "column_order": 2
        }
      ]
    },  
    {
      "action": "hunt domain",
      "identifier": "hunt_domain",
      "description": "Look for information about a domain in the Intsights database.",
      "type": "investigate",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {
        "domain": {
          "description": "Domain to hunt",
          "data_type": "string",
          "contains": ["domain"],
          "required": true
        }
      },
      "render": {
        "width": 12,
        "title": "Hunt Domain",
        "menu_name": "Intsights",
        "type": "table",
        "height": 5
      },
      "output": [
        {
          "data_path": "action_result.data.*.Value",
          "data_type": "string",
          "contains": ["domain"],
          "column_name": "Domain",
          "column_order": 0
        },
        {
          "data_path": "action_result.data.*.Source",
          "data_type": "string",
          "column_name": "Source",
          "column_order": 1
        },
        {
          "data_path": "action_result.data.*.Description",
          "data_type": "string",
          "column_name": "Description",
          "column_order": 2
        }
      ]
    },  
    {
      "action": "hunt ip",
      "identifier": "hunt_ip",
      "description": "Look for information about an IP in the Intsights database.",
      "type": "investigate",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {
        "ip": {
          "description": "IP to hunt",
          "data_type": "string",
          "contains": ["ip"],
          "required": true
        }
      },
      "render": {
        "width": 12,
        "title": "Hunt Ip",
        "menu_name": "Intsights",
        "type": "table",
        "height": 5
      },
      "output": [
        {
          "data_path": "action_result.data.*.Value",
          "data_type": "string",
          "contains": ["ip"],
          "column_name": "IP",
          "column_order": 0
        },
        {
          "data_path": "action_result.data.*.Source",
          "data_type": "string",
          "column_name": "Source",
          "column_order": 1
        },
        {
          "data_path": "action_result.data.*.Description",
          "data_type": "string",
          "column_name": "Description",
          "column_order": 2
        }
      ]
    },  
    {
      "action": "hunt url",
      "identifier": "hunt_url",
      "description": "Look for information about a URL in the Intsights database.",
      "type": "investigate",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {
        "url": {
          "description": "URL to hunt",
          "data_type": "string",
          "contains": ["url"],
          "required": true
        }
      },
      "render": {
        "width": 12,
        "title": "Hunt Url",
        "menu_name": "Intsights",
        "type": "table",
        "height": 5
      },
      "output": [
        {
          "data_path": "action_result.data.*.Value",
          "data_type": "string",
          "contains": ["url"],
          "column_name": "URL",
          "column_order": 0
        },
        {
          "data_path": "action_result.data.*.Source",
          "data_type": "string",
          "column_name": "Source",
          "column_order": 1
        },
        {
          "data_path": "action_result.data.*.Description",
          "data_type": "string",
          "column_name": "Description",
          "column_order": 2
        }
      ]
    },
    {
      "action": "on poll",
      "identifier": "on_poll",
      "description": "Callback action for the on_poll ingest functionality.",
      "type": "ingest",
      "read_only": true,
      "versions": "EQ(*)",
      "parameters": {
        "container_id": {
          "data_type": "string",
          "order": 0,
          "description": "Container IDs to limit the ingestion to."
        },
        "start_time": {
          "data_type": "numeric",
          "order": 1,
          "description": "Start of time range, in epoch time (milliseconds)",
          "verbose": "If not specified, the default is past 10 days"
        },
        "end_time": {
          "data_type": "numeric",
          "order": 2,
          "description": "End of time range, in epoch time (milliseconds)",
          "verbose": "If not specified, the default is now"
        },
        "container_count": {
          "data_type": "numeric",
          "order": 3,
          "description": "Maximum number of container records to query for."
        },
        "artifact_count": {
          "data_type": "numeric",
          "order": 4,
          "description": "Maximum number of artifact records to query for."
        }
      },
      "output": []
    }
  ]
}